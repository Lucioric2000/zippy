#Listen 9999
#<VirtualHost *:9999>
<VirtualHost *:801>
	ServerAdmin dbrawand@nhs.net
    SetEnv FLASK_ENV development
    WSGIDaemonProcess zippy user=apache group=apache threads=5 home=/srv/qgen/zippy/
    WSGIScriptAlias / /var/www/zippy/zippy_dev.wsgi

    <Directory /var/www/zippy>
        WSGIProcessGroup zippy
        WSGIApplicationGroup %{GLOBAL}
        WSGIScriptReloading On
        Order deny,allow
        Allow from all
    </Directory>
    <Directory /srv/qgen/zippy>
        WSGIProcessGroup zippy
        WSGIApplicationGroup %{GLOBAL}
        WSGIScriptReloading On
        Order deny,allow
        Allow from all
    </Directory>

</VirtualHost>
<VirtualHost 173.212.225.155:801>
    ServerAdmin dbrawand@nhs.net
    ServerName bioinfojdp.freelancecuernavaca.com
    #SetEnv FLASK_ENV development
    SetEnv FLASK_DEBUG 1
    WSGIDaemonProcess zippy_developer user=apache group=apache threads=5 home=/srv/qgen/zippy/
    WSGIScriptAlias / /var/www/zippy/zippy_dev.wsgi

    <Directory /var/www/zippy>
        WSGIProcessGroup zippy_developer
        WSGIApplicationGroup %{GLOBAL}
        WSGIScriptReloading On
        Order deny,allow
        Allow from all
    </Directory>
    <Directory /srv/qgen/zippy>
        WSGIProcessGroup zippy_developer
        WSGIApplicationGroup %{GLOBAL}
        WSGIScriptReloading On
        Order deny,allow
        Allow from all
    </Directory>
</VirtualHost>

